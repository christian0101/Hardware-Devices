<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <top>
        <Label fx:id="welcomeLabel" text="Welcome" BorderPane.alignment="TOP_CENTER" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0">
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin></Label>
    </top>
    <left>
        <VBox>
            <Label>Previous Data</Label>
            <ListView fx:id="listView" prefHeight="148.0" prefWidth="73.0" />
            <Label fx:id="dataLabel" text="Data" GridPane.columnIndex="0" GridPane.rowIndex="1" />

        </VBox>
    </left>
    <center>
        <VBox alignment="bottom_right" spacing="10" BorderPane.alignment="TOP_CENTER">

            <Label text="Current Data will be shown here: " />

            <Button text="Start exercise" />
         <padding>
            <Insets bottom="20.0" right="100.0" />
         </padding>
        </VBox>
    </center>

</BorderPane>
